<%- include("header") -%>

      <div class="box" id="heading">
        <h1><%= listTitle %></h1>
     </div>

     <div class="box">
        <% for(var i=0;i<newItems.length;i++){ %>
          <div class="item">
            <input type="checkbox">
            <p> <%= newItems[i] %></p>
          </div>
       <% } %>
       <form class="item" action="/" method="POST">
        <input type="text" name="userdata" placeholder="New Work" required autocomplete="off">
        <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>
     </div>
      
<%- include("footer") -%>
      
